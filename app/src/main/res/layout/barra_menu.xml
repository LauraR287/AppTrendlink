<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- MENU -->
    <ImageView
        android:id="@+id/inicio"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/hogar"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="680dp" />

    <ImageView
        android:id="@+id/buscar"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/busqueda"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="680dp" />

    <ImageView
        android:id="@+id/diseniar"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/diseniar"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteX="195dp"
        tools:layout_editor_absoluteY="680dp" />

    <ImageView
        android:id="@+id/mensajes"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/mensajes"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteX="271dp"
        tools:layout_editor_absoluteY="680dp" />

    <ImageView
        android:id="@+id/perfil"
        android:layout_width="30dp"
        android:layout_height="43dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/usuario"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteX="330dp"
        tools:layout_editor_absoluteY="665dp" />


</androidx.constraintlayout.widget.ConstraintLayout>